{
  "name": "react-apollo-spa-cucumber",
  "description": "Useful boilerplate to use and start developing a scalable app using React, Apollo, Webpack, Hot reloading, etc...",
  "author": "Ricardo MartÃ­nez",
  "license": "MIT",
  "version": "0.9.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Richacinas/react-apollo-spa-cucumber"
  },
  "homepage": "https://github.com/Richacinas/react-apollo-spa-cucumber",
  "keywords": [
    "react",
    "webpack",
    "express",
    "hot reloading",
    "react-hot-reloader",
    "apollo",
    "starter",
    "boilerplate",
    "babel"
  ],
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run watch-client\"",
    "watch-client": "cross-env NODE_ENV=development PORT=3000 webpack-dev-server --mode development --open --config webpack/webpack-dev-server.js",
    "build": "cross-env NODE_ENV=production webpack --colors --display-error-details --config=webpack/prod.config.js",
    "postinstall": "npm run build",
    "prettier": "prettier-eslint --write --eslint-config-path .eslintrc '**/*.js'",
    "lint": "eslint .",
    "lighthouse-report": "lighthouse http://localhost:8080 --view"
  },
